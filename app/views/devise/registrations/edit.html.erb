<h2><%= resource_name.to_s.humanize %> info</h2>

<%= form_with model: @user do |f| %>
<!--%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>-->
  <!--%= render "devise/shared/error_messages", resource: resource %>-->
  
  <label for = "inputName">Name</label><br>
    <%= f.text_field :name, autofocus: true, id:"inputName", placeholder: "name" %><br>
  <label for = "inputImage">Image</label><br>
    <%= f.file_field :profile_image, placeholder: "profile_image", accept: "image/*" %><br>
  <label for = "inputIntroduction">Introduction</label><br>
    <%= f.text_field :introduction %><br>
    <%= f.submit "Update User" %>

<% end %>
